<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!--============================== Doors ==============================-->
	<ThingDef ParentName="DoorBase">
		<defName>RE_PrefabDoorColonial</defName>
		<label>prefab colonial autodoor</label>
		<description>Divides rooms. Powered operation allows people to move through the door without slowing down.</description>
		<thingClass>RimEffect.Building_AutoDoorLockable</thingClass>
		<graphicData>
			<texPath>Things/Building/Door/PrefabDoorColonial_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<statBases>
			<WorkToBuild>350</WorkToBuild>
			<Beauty>2</Beauty>
		</statBases>
		<uiIconPath>Things/Building/Door/PrefabDoorColonial_MenuIcon</uiIconPath>
		<costList>
			<Steel>10</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<costStuffCount>15</costStuffCount>
		<comps>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
				<basePowerConsumption>25</basePowerConsumption>
			</li>
			<li Class="CompProperties_Breakdownable"/>
		</comps>
		<building>
			<blueprintGraphicData>
				<texPath>Things/Building/Door/PrefabDoorColonial_Blueprint</texPath>
			</blueprintGraphicData>
		</building>
		<researchPrerequisites>
			<li>RE_BasicPrefabrication</li>
		</researchPrerequisites>
		<stuffCategories Inherit='False'>
		  <li>Metallic</li>
		</stuffCategories>
		<constructionSkillPrerequisite>6</constructionSkillPrerequisite>
	</ThingDef>
	
