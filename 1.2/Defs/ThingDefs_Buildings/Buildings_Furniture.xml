  <ThingDef ParentName="FurnitureWithQualityBase">
    <defName>RE_PrefabHoloTable</defName>
    <label>holo table</label>
    <description>People eat off tables when chairs are placed facing them.</description>
    <graphicData>
      <texPath>Things/Building/Furniture/PrefabHoloTable</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(4.8,4.8)</drawSize>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <thingClass>Building</thingClass>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
<designationCategory>Joy</designationCategory>
    <pathCost>42</pathCost>
    <fillPercent>0.4</fillPercent>
    <thingCategories>
      <li>BuildingsJoy</li>
    </thingCategories>
    <socialPropernessMatters>true</socialPropernessMatters>
    <canOverlapZones>false</canOverlapZones>
    <leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
    <staticSunShadowHeight>0.20</staticSunShadowHeight>
    <uiIconScale>0.8</uiIconScale>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToBuild>1500</WorkToBuild>
      <Mass>10</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>1</Beauty>
    </statBases>
    <size>(3,3)</size>
    <costStuffCount>50</costStuffCount>
    <rotatable>false</rotatable>
    <building>
      <joyKind>Television</joyKind>
      <spawnedConceptLearnOpportunity>TVForSickPeople</spawnedConceptLearnOpportunity>
      <boughtConceptLearnOpportunity>TVForSickPeople</boughtConceptLearnOpportunity>
      <watchBuildingStandDistanceRange>2~4</watchBuildingStandDistanceRange>
      <watchBuildingStandRectWidth>9</watchBuildingStandRectWidth>
      <watchBuildingStandRectHight>9</watchBuildingStandRectHighth>
      <watchBuildingInSameRoom>true</watchBuildingInSameRoom>
      <effectWatching>WatchingTelevision</effectWatching>
    </building>
    <placeWorkers>
      <li>PlaceWorker_WatchArea</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>200</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
    </comps>
  </ThingDef>

</Defs>