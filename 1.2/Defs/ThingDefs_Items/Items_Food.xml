<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="MealCooked">
		<defName>RE_MealColonial</defName>
		<label>colonial meal</label>
		<description>An extremely variety meal focusing on variety and over quantity. Commonly found in alliance colonies.</description>
		<graphicData>
			<texPath>Things/Item/Meal/Colonial</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>22</MarketValue>
			<WorkToMake>200</WorkToMake>
			<Nutrition>0.8</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealLavish</preferability>
			<tasteThought>AteLavishMeal</tasteThought>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
		</ingestible>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>0.75</daysToRotStart>
				<!-- short rot time, you want to make them as needed not store them -->
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
	</ThingDef>

</Defs>